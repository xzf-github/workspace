        mc:Ignorable="d"
        Title="徐泽枫_MainWindow" Height="350" Width="525">
    
     <StackPanel Background="LightBlue">
        <TextBlock x:Name="textBox1" Height="50" Margin="4" Background="LightCoral"/>
